                </div>
            </div>
            <div class="content" style="height:1045px">{{#if is_service_record}}<h5
                    style="font-size:23px;font-weight:700;margin-top:20px;text-align:left">Détails des coûts de service</h5>{{/si}}
                {{#if is_service_record}}<div
                    style="display:flex;flex-wrap:wrap;border:1px solid #000;border-radius:4px">
                    <div style="background:#0a5094;width:100%;display:flex;color:#fff">
                        <div style="width:20%;padding:18px">S. Non</div>
                        <div
                            style="width:60%;padding:18px;border-left:1px solid #d7d7d7;border-right:1px solid #d7d7d7">Service</div>
                        <div style="width:20%;padding:18px">Prix</div>
                    </div>{{#each ServiceCord}}<div class="serviceTable"
