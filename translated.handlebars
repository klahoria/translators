                        <div style="width:20%;padding:18px">{{indice}}</div>
                        <div
                            style="width:60%;padding:18px;border-left:1px solid #d7d7d7;border-right:1px solid #d7d7d7;word-break:break-all">{{service}}</div>
                        <div style="width:20%;padding:18px;word-break:break-all">{{prix}}</div>
                    </div>{{/chaque}}</div>{{/si}}
                {{#if show_first_page_payments}}<div>
                    <h4 style="font-size:23px;font-weight:700;margin-top:20px;text-align:left">Détails de paiement</h4>
                    <div
                        style="width:100%;margin-top:20px;border-radius:4px;background-color:#fff;padding:0;border:1px solid #000">
                        <table style="border-collapse:collapse;width:100%">
                            <thead>
                                <tr style="font-size:12px;color:#fff;background:#0a5094">{{#if from_business}}<th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:normalborder-right: 1px solid #d7d7d7">Date récurrente</th>{{/si}}<th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">Date de transaction</th>
                                    <th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">Statut</th>
                                    <th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">Type de paiement</th>
                                    <th style="padding:18px;text-align:left;font-size:14px;font-weight:400">Transaction
                                        Montant</th>
                                </tr>
                            </thead>
                            <tbody>{{#each firstPagePayments}}<tr
                                    style="font-size:18px;color:#000;border-top:1px solid #d7d7d7">{{#if from_business}}<th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">{{PAYS_DATE}}</th>{{/si}}<th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">{{date_added}}</th>
                                    <th
                                        style="padding:18px;text-align:left;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">{{PAYS_STATUS}}</th>
                                    <th
                                        style="padding:18px;text-align:left;text-transform:capitalize;font-size:14px;font-weight:400;border-right:1px solid #d7d7d7">{{pt_name}}</th>
                                    <th
                                        style="padding:18px;text-align:left;text-transform:capitalize;font-size:14px;font-weight:400">{{montant déduit}}</th>
                                </tr>{{/chaque}}</tbody>
                        </table>
                    </div>{{#if is_only_one_payment_pages}}<div style="margin-top:5px;font-size:16px"><b>Remarque: tous les
                            Les paiements seront affichés en tant que {{Statement_Descriptor}} sur votre instruction bancaire / carte.</b>
                    </div>{{/si}}</div>{{/si}}</div>
